{
  "name": "@rocketsofawesome/mirage",
  "version": "0.1.54",
  "main": "dist/bundle.js",
  "dependencies": {
    "cloudinary-core": "^2.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "export NODE_ENV=development && ./node_modules/.bin/styleguidist server --open",
    "build": "export NODE_ENV=development && ./node_modules/.bin/styleguidist build",
    "prepublish": "export NODE_ENV=development && ./node_modules/.bin/rollup --c",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "npm run test -- --watch --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.12.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "install": "^0.12.1",
    "jest": "^20.0.4",
    "jest-enzyme": "^7.0.0",
    "jest-styled-components": "^6.2.1",
    "npm": "^6.4.1",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "^8.0.2",
    "raf": "^3.4.1",
    "react-dev-utils": "^7.0.1",
    "react-styleguidist": "^7.3.11",
    "react-test-renderer": "^16.5.2",
    "resolve": "^1.9.0",
    "rollup": "^0.65.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/index.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.base.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "module": "src/index.js",
    "moduleNameMapper": {
      "^SRC(.*)$": "<rootDir>/src$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "keywords": [
    "react",
    "styled-components",
    "react-component",
    "rockets-of-awesome"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketsofawesome/mirage.git"
  },
  "description": "[Live Demo of the Pattern Library](https://rocketsofawesome.github.io/mirage/)",
  "bugs": {
    "url": "https://github.com/rocketsofawesome/mirage/issues"
  },
  "homepage": "https://rocketsofawesome.github.io/mirage/",
  "author": "Shawn Mealey",
  "license": "MIT"
}
